        {HardPlastic 0.000 0.560 0.280 0.690 1.000 0.000 0.000 0.000}
        {MetallicPastel 0.000 0.260 0.550 0.190 1.000 0.000 0.000 0.000}
        {Steel 0.250 0.000 0.380 0.320 1.000 0.000 0.000 0.000}
        {Translucent 0.000 0.700 0.600 0.300 0.800 0.000 0.000 0.000}
        {Edgy 0.000 0.660 0.000 0.750 1.000 0.620 0.940 0.000}
        {EdgyShiny 0.000 0.660 0.960 0.750 1.000 0.760 0.940 0.000}
        {EdgyGlass 0.000 0.660 0.500 0.750 0.620 0.620 0.940 0.000}
        {Goodsell 0.520 1.000 0.000 0.000 1.000 4.000 0.900 0.000}
        {AOShiny 0.000 0.850 0.200 0.530 1.000 0.000 0.000 0.000}
        {AOChalky 0.000 0.850 0.000 0.530 1.000 0.000 0.000 0.000}
        {AOEdgy 0.000 0.900 0.200 0.530 1.000 0.620 0.930 0.000}
        {BlownGlass 0.040 0.340 1.000 1.000 0.100 0.000 0.000 1.000}
        {GlassBubble 0.250 0.340 1.000 1.000 0.040 0.000 0.000 1.000}
      } {
        set i 1
        foreach prop $properties { material change $prop [lindex $ele 0] [lindex $ele $i]; incr i }
