    if { 0 } {
      set matsVMD [material list]
      set matsVMDPREFS {Opaque Transparent BrushedMetal Diffuse Ghost Glass1 Glass2 Glass3 Glossy HardPlastic MetallicPastel Steel Translucent Edgy EdgyShiny EdgyGlass Goodsell AOShiny AOChalky AOEdgy BlownGlass GlassBubble}
      # deletions
      foreach ele $matsVMD { if { [lsearch $matsVMDPREFS $ele] == -1 } { material delete $ele } }
      # additions
      foreach ele $matsVMDPREFS { if { [lsearch $matsVMD GlassBubble] == -1 } { material add $ele } }
